{
  "patientLists": [
    {
      "url": "${MAINZELLISTE_URL}",
      "oAuthConfig": {
        "url": "${KEYCLOAK_URL}",
        "realm": "${KEYCLOAK_REALM}",
        "clientId": "${KEYCLOAK_CLIENT_ID}"
      },
      "roles": [
        {
          "name": "admin",
          "permissions": {
            "resources": {
              "patient": {
                "operations": [ "C", "R", "U", "D" ]
              },
              "consent": {
                "operations": [ "C", "R", "U" ]
              }
            }
          }
        },
        {
          "name": "study-nurse",
          "permissions": {
            "realm": {
              "name": "project",
              "criteria": {
                "ids": ["projectId"]
              }
            },
            "resources": {
              "patient": {
                "operations": [ "C", "R" ],
                "contents": {
                  "ids": [
                    {
                      "type": "projectId",
                      "operations": [ "C", "R" ]
                    },
                    {
                      "type": "clinicExtId",
                      "operations": [ "C", "R", "U" ]
                    }
                  ]
                }
              },
              "consent": {
                "operations": [ "C", "R"]
              }
            }
          }
        }
      ],
      "mainIdType": "${ML_UI_MAIN_ID_TYPE}",
      "showAllIds": ${ML_UI_SHOW_ALL_IDS},
      "fields": [
        { "i18n": "first_name_text", "name": "Vorname", "mainzellisteField": "vorname"},
        { "i18n": "last_name_text", "name": "Nachname", "mainzellisteField": "nachname"},
        { "i18n": "birth_name_text", "name": "Geburtsname", "mainzellisteField": "geburtsname"},
        { "i18n": "birth_date_text", "name": "Geburtdatum", "mainzellisteFields": ["geburtstag", "geburtsmonat", "geburtsjahr"]},
        { "i18n": "residence_text", "name": "Wohnort", "mainzellisteField": "plz"},
        { "i18n": "zip_code_text", "name": "PLZ", "mainzellisteField": "ort"}
      ],
      "debug": ${ML_UI_DEBUG_MODE},
      "betaFeatures": {
        "consent": ${ML_UI_ENABLE_CONSENT},
        "copyConcatenatedId": false,
        "copyId": true
      },
      "associatedIdGroups": [
        { "name": "lab", "idTypes": [{"i18n": "ext_id_text", "name": "extId"}, {"i18n": "int_id_text", "name": "intId"}]},
        { "name": "visit", "idTypes": [{"i18n": "ext_id_text", "name": "extId"}, {"i18n": "int_id_text", "name": "intId"}]},
        { "name": "cache", "idTypes": [{"i18n": "input_id_text", "name": "inputId"}, {"i18n": "output_id_text", "name": "outputId"}]},
        { "name": "temp", "idTypes": [{"i18n": "te_id_text", "name": "teId"}, {"i18n": "mp_id_text", "name": "mpId"}, {"i18n": "test", "name": "test"}]}
      ]
    }
  ]
}
